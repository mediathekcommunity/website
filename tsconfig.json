{
  "extends": "astro/tsconfigs/strict",
  "include": ["src/**/*", "src/env.d.ts", "src/types/global.d.ts"],
  "exclude": ["dist", "node_modules/.svelte2tsx-language-server-files"],
  "compilerOptions": {
    "jsx": "preserve",
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "types": ["svelte", "astro/client"]
  }
}
